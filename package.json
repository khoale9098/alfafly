{
  "name": "alphafly",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/khoale9098/alphafly.git",
  "author": "<khoale9098@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "find:unused": "next-unused"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "arrowParent": "always",
    "singleQuote": true,
    "printWidth": 120,
    "endOfLine": "auto"
  },
  "dependencies": {
    "autoprefixer": "^10.2.1",
    "classnames": "^2.2.6",
    "next": "^10.0.5",
    "postcss": "^8.2.4",
    "postcss-nesting": "^7.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "styled-jsx": "^3.4.1",
    "tailwindcss": "^2.0.2"
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@container/*": [
        "container/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.5",
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/styled-jsx": "^2.2.8",
    "next-unused": "^0.0.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
